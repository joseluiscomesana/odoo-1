<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="home_page" model="website.page">
        <field name="name">Home</field>
        <field name="website_published">True</field>
        <field name="url">/home</field>
        <field name="type">qweb</field>
        <field name="key">theme_georedia.home</field>
        <field name="arch" type="xml">
            <t t-name="theme_georedia.home_template">
                <t t-call="website.layout">
                    <div id="wrap">
                        <div class="container">
                            <h1>Home GeoRedia</h1>
                            <ul class="home">
                                <li>Home</li>
                                <li>Porqué nosotros</li>
                                <li>Diseña</li>
                                <li>Contacto</li>
                            </ul>
                        </div>
                        <div class="oe_structure" />
                    </div>
                </t>
            </t>
        </field>
    </record>

    <record id="home_page_link" model="website.menu">
        <field name="name">Home</field>
        <field name="page_id" ref="home_page"/>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">99</field>
    </record>


</odoo>
