<odoo>
    <data>
        <template id="selec">
          <t t-call="website.layout">
            <t t-set="title">Presupuestos GeoRedia</t>
              <div class="oe_structure">
                <div class="container">
                  <select id="presupuesto">
                    <t t-foreach="objects" t-as="object">
                      <option t-attf-value="{{presupuestos['id']}}"><t t-esc="presupuestos['ensayo']"></option>
                    </t>
                  </select>
                </div>
              </div>
            </t>
          </t>
        </template>
    </data>
</odoo>