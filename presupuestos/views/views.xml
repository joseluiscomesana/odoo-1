    <!-- explicit list view definition -->
    <!-- view tree -->
    <record id="view_presupuestos_presupuesto_tree" model="ir.ui.view">
      <field name="name">view.presupuestos.presupuesto.tree</field>
      <field name="model">presupuestos.presupuesto</field>
      <field name="arch" type="xml">
        <tree string="Presupuesto">
          <field name="ensayo" />
          <field name="descripcion" />
        </tree>
      </field>
    </record>
    <!-- view kanban -->
    <record id="view_presupuestos_presupuesto_kanban" model="ir.ui.view">
      <field name="name">view.presupuestos.presupuesto.kanban</field>
      <field name="model">presupuestos.presupuesto</field>
      <field name="arch" type="xml">
        <kanban class="o_presupuestos_presupuesto_kanban">
          <field name="id"/>
          <field name="ensayo"/>
          <field name="descripcion"/>
          <templates>
            <t t-name="kanban-box">
              <div t-attf-class="oe_kanban_global_click">
                <div class="o_kanban_image">
                  <img alt="Avatar" 
                        t-att-src="kanban_image('presupuestos.presupuesto', 'image', record.id.raw_value)"/>
                </div>
                <div class="oe_kanban_details">
                  <strong class="o_kanban_record_title">
                    <field name="ensayo"/>
                  </strong>
                  <div t-if="record.descripcion.value">
                    <t t-esc="record.descripcion.value"/>
                  </div>
                </div>
              </div>
            </t>
          </templates>
        </kanban>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record id="action_presupuestos_presupuesto_view" model="ir.actions.act_window">
      <field name="name">Presupuesto</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">presupuestos.presupuesto</field>
      <field name="view_mode">tree,kanban</field>
      <!-- <field name="search_view_id">view.presupuestos.presupuesto.search</field> -->
      <field name="context" eval="{'search_default_filter_by_ensayo':1,'search_default_filter_by_descripcion':2}"/>
      <field name="help" type="html">
        <p class="oe_view_nocontent_create">
          Presupuestos
        </p>
        <p>
          No existen presupuestos creados
        </p>
      </field>
    </record>

